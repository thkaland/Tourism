<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>  
        <welcome-file>toCountry</welcome-file>  
    </welcome-file-list> 
    <servlet>
        <servlet-name>toCountry</servlet-name>
        <servlet-class>toCountry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fromCountry</servlet-name>
        <servlet-class>fromCountry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>result</servlet-name>
        <servlet-class>result</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>toCountry</servlet-name>
        <url-pattern>/toCountry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fromCountry</servlet-name>
        <url-pattern>/fromCountry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
